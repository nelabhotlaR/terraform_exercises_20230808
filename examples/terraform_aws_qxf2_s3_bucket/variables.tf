
variable "AWS_REGION" {
  description = "AWS region"
  type        = string
  default     = "region-name"
}

variable "AWS_ACCESS_KEY_ID" {
  description = "AWS access key"
  type        = string
  default     = "access-key-id"
}

variable "AWS_SECRET_ACCESS_KEY" {
  description = "AWS secret access key"
  type        = string
  default     = "secret-access-key"
}
